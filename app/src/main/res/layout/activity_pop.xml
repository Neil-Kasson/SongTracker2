<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".pop" android:background="#9F9EA2">

    <TextView
            android:text="Song Entered"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            android:textColor="#000000"
            android:gravity="center"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="50dp"
            android:layout_marginStart="50dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="50dp"
            android:layout_marginRight="50dp"
            android:layout_marginTop="100dp"
            app:layout_constraintTop_toTopOf="parent"
            android:textSize="24sp"
            app:fontFamily="@font/cinzel_decorative_bold" android:textStyle="bold"/>
    <Button
            android:text="Go Back"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/back2"
            style="@android:style/Widget.Holo.Button.Inset"
            android:textColor="#000000"
            android:layout_marginTop="75dp"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="100dp"
            android:layout_marginRight="100dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="100dp"
            android:layout_marginStart="100dp" android:paddingTop="20dp" android:paddingBottom="22dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/show" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/textView5" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="50dp" android:layout_marginStart="50dp"
            app:fontFamily="@font/alice"/>
</androidx.constraintlayout.widget.ConstraintLayout>